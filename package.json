{
  "name": "eosjs-ecc",
  "version": "1.0.5",
  "description": "Elliptic curve cryptography functions",
  "keywords": "ECC, Private Key, Public Key, Signature, AES, Encryption, Decryption",
  "main": "lib/index.js",
  "scripts": {
    "build": "mkdir -p dist && browserify -o dist/eosjs-ecc.js -s eosjs_ecc index.js && sha256sum dist/eosjs-ecc.js",
    "prepublish": "node_modules/babel-cli/bin/babel.js src --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/EOSIO/eosjs-ecc.git"
  },
  "dependencies": {
    "bigi": "^1.4.2",
    "bn.js": "^4.11.6",
    "browserify-aes": "^1.0.6",
    "bs58": "^4.0.1",
    "bytebuffer": "^5.0.1",
    "create-hash": "^1.1.3",
    "create-hmac": "^1.1.6",
    "ecurve": "^1.0.5",
    "more-entropy": "^0.0.7",
    "secure-random": "^1.1.1"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^14.4.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
